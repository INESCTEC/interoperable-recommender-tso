# General configs:
POST_TO_ENERGY_APP=1
POST_ONLY_ON_UPDATES=1

# Entsoe configs:
ENTSOE_API_KEY=insert_api_key_here

# Database configs:
POSTGRES_HOST=db_host
POSTGRES_DB=db_name
POSTGRES_USER=db_user
POSTGRES_PASSWORD=db_password
POSTGRES_PORT=db_port

# EnergyAPP backend configs:
ENERGYAPP_HOST=host_here
ENERGYAPP_PORT=port_here
ENERGYAPP_N_RETRIES=3
ENERGYAPP_APIKEY=insert_api_key_here
ENERGYAPP_BASEPATH=/energy-app/api/v2